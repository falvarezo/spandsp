    <xs:element ref='t:acceptance-tone'/>
    <xs:element ref='t:executive-override-tone'/>
    <xs:element ref='t:function-acknowledge-tone'/>
    <xs:element ref='t:queue-tone'/>
    <xs:element ref='t:valid-tone-radio-paging'/>
    <xs:element ref='t:notify-tone'/>
    <xs:element ref='t:preemption-tone'/>
    <xs:element ref='t:comfort-tone'/>
    <xs:element ref='t:connection-tone'/>
    <xs:element ref='t:refusal-tone'/>
    <xs:element ref='t:line-lockout-tone'/>
    <xs:element ref='t:permanent-signal-tone'/>
    <xs:element ref='t:identification-tone'/>
    <xs:element ref='t:positive-indication-tone'/>
    <xs:element ref='t:negative-indication-tone'/>
   </xs:choice>
   <xs:attribute name='country' type='string' use='required'/>
   <xs:attribute name='uncode' type='string' use='required'/>
  </xs:complexType>
 </xs:element>

