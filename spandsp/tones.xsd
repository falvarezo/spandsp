    <xs:element ref='t:service-activated-tone'/>
    <xs:element ref='t:record-tone'/>
    <xs:element ref='t:facilities-tone'/>
    <xs:element ref='t:offering-tone'/>
    <xs:element ref='t:route-tone'/>
    <xs:element ref='t:confirmation-tone'/>
    <xs:element ref='t:intrusion-tone'/>
    <xs:element ref='t:intercept-tone'/>
